                </select>
                <!-- <span class="spanSearch"> Qualification </span> 
                <select class="selectI">
                 <option value="">Choisire un Qualification</option>
                            <option value="">Tout les Qualification</option>
                </select>
                                <span class="spanSearch"> Class </span>               
                <select class="selectI"  id="Classe">
                   <option value="">Choisire un Class</option>
                </select> -->
                <input type="button" class="btn_plus" value="+"/>&nbsp;
                <input  id='owm_"+indexOItmes+"' type='button' class='btn_moin_1' value='-'/>
              </div>
       
            </div>
             <span id="search_container">
                <!-- <div class="rowm">
                </div> -->
                
             </span>
            <div class="rowm">
              <span>Mots clés :</span>
              <div class="fieldsm">
                <input type="text" id="Cle1" />
                <select name="EtOu1" id="EtOu1">
                  <option value="AND">et</option>
                </select>
                <input type="text" id="Cle2" />
                <select name="EtOu2" id="EtOu2">
                  <option value="AND">et</option>
                </select>
                <input type="text" id="Cle3" />
              </div>
            </div>

            <div class="rowm">
              <span>Date limite :</span>
              <div class="fieldsm">
                <label for="Datentre">Entre  :</label>
                <input type="date" name="Datentre" id="Datentre" />
                <label for="Datet">Et  :</label>
                <input type="date" name="Datet" id="Datet" />
              </div>
            </div>

            <div class="rowm">
              <span>Caution :</span>
              <div class="fieldsm">
                <label for="Cauentre">Entre  :</label>
                <input type="number" name="Cauentre" id="Cauentre" min="0" />
                <label for="Cauet">Et  :</label>
                <input type="number" name="Cauet" id="Cauet" min="0" />
              </div>
            </div>

            <div class="rowm">
              <span>Budget :</span>
              <div class="fieldsm">
                <label for="Budentre">Entre  :</label>
                <input type="number" name="Budentre" id="Budentre" min="0" />
                <label for="Budet">Et  :</label>
                <input type="number" name="Budet" id="Budet" min="0" />
              </div>
            </div>

            <div class="rowm">
              <span>N° d'ordre  :</span>
              <div class="fieldsm">
                <input type="text" name="ordre" id="ordre"  />
              </div>
            </div>

            <div class="rowm">
              <span>Référence  :</span>
              <div class="fieldsm">
                <input type="text" name="ref" id="ref" />
              </div>
            </div>
