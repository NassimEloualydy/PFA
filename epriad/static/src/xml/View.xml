            <div  class="rowm">
               <div class="fieldsm">
                <!-- <input type="text" name="Domain" id="Domain" /> -->
                 <span class="spanSearch"> Domaine </span> 
                 <select id="select0" class="selectIDomain"   >
                 <option value="">Tous les domaines</option>
                 <option value="1">Equipement</option>
                 <option value="2">Habitat</option>

                </select>
                <span class="spanSearch"> Secteur </span> 

                <select  id="secteurselect0" class="selectI sectuers">
